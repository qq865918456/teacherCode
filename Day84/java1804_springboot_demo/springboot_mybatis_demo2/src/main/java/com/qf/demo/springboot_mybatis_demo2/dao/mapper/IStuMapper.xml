<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qf.demo.springboot_mybatis_demo2.dao.IStuDao" >
    <select id="queryAll" resultType="student">
        select * from student
    </select>

    <insert id="insert">
        insert into student value(null, #{name}, #{age}, #{cid})
    </insert>
</mapper>